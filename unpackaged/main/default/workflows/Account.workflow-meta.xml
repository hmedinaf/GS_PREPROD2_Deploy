    <rules>
        <fullName>Afluente a Prospecto</fullName>
        <actions>
            <name>act_aflue_a_prosp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>RecordType.Name = &quot;Afluente&quot; &amp;&amp; 
FechaAsignacion__c &lt; TODAY() &amp;&amp;
OR(
ISNEW(),
ISCHANGED(Name),
ISCHANGED(Nombre_cta__c),
ISCHANGED(Apellidopaterno_cta__c),
ISCHANGED(Apellidomaterno_cta__c),
ISCHANGED(NSS__c), 
ISCHANGED(Phone), 
ISCHANGED(TelefonoCelular__c),
ISCHANGED(Sexo__c),
ISCHANGED(TipoCredito__c),
ISCHANGED(SubCanalVenta__c),
ISCHANGED(Fraccionamiento_cta__c)  
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
