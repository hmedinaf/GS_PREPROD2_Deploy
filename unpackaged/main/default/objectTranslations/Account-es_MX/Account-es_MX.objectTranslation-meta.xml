    <validationRules>
        <errorMessage><!-- La dirección es obligatoria para los Afluentes, Favor de revisar que la dirección este completa. --></errorMessage>
        <name>Afluente_Direccion_Obligatoria</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se permite asignar prospectador si ya tiene Asesor asignado. --></errorMessage>
        <name>Afluente_no_Prospectador_si_Asesor</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede modificar el Colaborador Javer, pues aún no termina su periodo de vigencia. --></errorMessage>
        <name>Cambio_Colaborador_Javer_Vigente</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede modificar el Check Apellido si la Cuenta ya tiene un Proceso de Venta --></errorMessage>
        <name>Check_Apellido_no_modificable</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede usar el valor de Programas Institucionales en Enlace, favor de cambiar la selección de la lisa. --></errorMessage>
        <name>ENLACE_No_Valores_CanalVenta</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede asignar un Colaborador Javer a la cuenta pues ésta ya cuenta con un proceso de venta --></errorMessage>
        <name>Edicion_Colaborador_Javer</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La Etapa de Seguimiento solo puede ser modificada cuando el estatus del cliente es POR ASIGNAR, ASIGNADO o POR REASIGNAR --></errorMessage>
        <name>Edicion_Etapa_Seguimiento</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No puede ser editada la Fecha de Afluencia --></errorMessage>
        <name>Edicion_Fecha_Afluencia</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- A una cuenta de tipo cliente no se le puede modificar la fecha de nacimiento --></errorMessage>
        <name>Edicion_Fecha_Nacimiento</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede editar el nombre de la cuenta pues ésta ya cuenta con un proceso de venta --></errorMessage>
        <name>Edicion_Nombre_Proceso_Venta</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La Ocupación solo puede ser indicada cuando el estatus del cliente es POR ASIGNAR, ASIGNADO o POR REASIGNAR --></errorMessage>
        <name>Edicion_Ocupacion</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede modificar el parentesco. --></errorMessage>
        <name>Edicion_Parentesco</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se puede asignar un recomendante pues el Sub canal de venta no es &apos;RECOMIENDA Y GANA&apos; ni la cuenta tiene un Lead Profiler asignado --></errorMessage>
        <name>Edicion_Recomendante</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La fecha de visita debe ser mayor o igual a la fecha de afluencia del fraccionamiento actual. --></errorMessage>
        <name>FechaVisita_MayorIgual_a_FechaAfluencia</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- La Fecha de afluencia debe ser 4 días previos o el día de hoy --></errorMessage>
        <name>Fecha_Afluencia_4DiasAntes</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Fecha de estimación No puede ser menor a la fecha de Precalificación --></errorMessage>
        <name>Fecha_estimada_vs_Fecha_Preca</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No tiene los permisos para modificar los campos de Nombre o Apellidos --></errorMessage>
        <name>Gerente_Vta_No_Modifica_Nombre</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Debe especificar la fecha estimada --></errorMessage>
        <name>Motivo_incuba_vs_Fecha_estimada</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Debe especificar la Capacidad de Compra --></errorMessage>
        <name>Motivo_incuba_vs_capacidad</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se permite Incubar la cuenta con un Proceso de Venta Activo --></errorMessage>
        <name>No_Incubar_con_Proceso_de_Venta</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No es posible modificar el Nombre del Cliente --></errorMessage>
        <name>No_Modificacion_Nombre_Cliente</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- No se permite asignar o cambiar Prospectador cuando el estatus Cliente es BAJA o INCUBADORA --></errorMessage>
        <name>No_Reasignar_Prospectador_en_Incubadora</name>
