<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Campos_Numericos</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Extension__c ,&quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorDisplayField>Extension__c</errorDisplayField>
    <errorMessage>Por Favor solo Ingresar Numeros</errorMessage>
</ValidationRule>
